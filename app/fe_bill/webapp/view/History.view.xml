<mvc:View
    xmlns:cards="sap.f.cards"
    xmlns:table="sap.ui.table"
    xmlns:f="sap.f"
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    controllerName="febill.controller.History"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page id="historyPage">
        <List
            id="historyList"
            items="{history>/value}"
            headerText="History"
        >
            <ObjectListItem title="{history>quantity}" />
        </List>
    </Page>
</mvc:View>
