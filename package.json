  "scripts": {
    "start": "cds-serve",
    "local": "cds watch",
    "hybrid": "cds watch --profile hybrid",
    "watch-fe_bill": "cds watch --open fe_bill/webapp/index.html?sap-ui-xx-viewCache=false",
